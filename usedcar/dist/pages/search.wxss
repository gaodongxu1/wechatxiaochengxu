
.allbacground {
  background-color: #F5F5F5;
}
.border {
  display: flex;
  border: 2px solid grey;
  padding: 3px 30px;
  background: white;
  width: 270px;
  height: 18px;
  border-radius: 18px;
}
.borders {
  height: 50px;
  width: 100%;
  border: 1px solid #F5F5F5;
}
.input {
  display: flex;
  margin: 10px 2px 3px 17px;
  font-size: 13px;
}
.search-found {
    font-size: 13px;
    color: gray;
    margin: 10px 0px 0px 10px;
}
.collect {
  display: flex;
  justify-content: space-between;
}
.collects {
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
